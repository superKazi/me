{
  "name": "kazis-website",
  "version": "1.0.0",
  "description": "Kaziâ€™s website",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js"
  },
  "keywords": [
    "personal",
    "website"
  ],
  "author": "Kazi Awal",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true,
      "node": true,
      "commonjs": true,
      "serviceworker": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "ignorePatterns": [
      "node_modules/**/*.js",
      "dist/**/.js"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "singleQuote": true
  },
  "browserslist": [
    "Chrome >= 61",
    "ChromeAndroid >= 80",
    "Safari >= 11",
    "iOS >= 11",
    "Firefox >= 60",
    "FirefoxAndroid >= 68",
    "Edge >= 16",
    "Opera >= 48",
    "Android >= 80",
    "Samsung >= 8.2"
  ],
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.12.1",
    "babel-loader": "8.1.0",
    "babel-preset-minify": "0.5.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.1.1",
    "core-js": "3.6.5",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "4.0.4",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "4.0.4",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.1.2",
    "script-ext-html-webpack-plugin": "2.1.5",
    "style-ext-html-webpack-plugin": "4.1.3",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.4",
    "webpack-pwa-manifest": "4.2.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "dependencies": {
    "mini-debounce": "1.0.8",
